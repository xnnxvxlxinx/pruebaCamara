<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.Nirjob" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Nirjob</name>
    <description>
        Nirjob APP
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Mateo Moscoso
    </author>
    <preference name="phonegap-version" value="cli-7.0.1" />
<preference name='pgb-builder-version' value='2' />
    <preference name="orientation" value="portrait" />
    <icon src="res/logo.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <engine name="ios" spec="~4.5.3" />
    <engine name="android" spec="~6.3.0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.4" />
    <plugin name="cordova-plugin-device" spec="~1.1.7" />
    <plugin name="cordova-plugin-camera" spec="~2.4.1" />
    <plugin name="cordova-plugin-compat" spec="^1.2.0" />
    <plugin name="cordova-plugin-file" spec="~4.3.3" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.6.3" />
    <plugin name="cordova-plugin-local-notification" spec="~0.8.4" />
    <plugin name="cordova-plugin-geolocation" spec="~2.4.3" />
    <plugin name="cordova.plugins.diagnostic" spec="~3.7.2" />
</widget>
